{
  "name": "ecommerce",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config dist/server.cjs",
    "dev": "nodemon -r dotenv/config src/server.js",
    "build": "esbuild src/server.js --bundle --platform=node --format=cjs --external:bcrypt --external:swagger-ui-express --external:swagger-ui-dist --outfile=dist/server.cjs",
    "prestart": "npm run build",
    "postinstall": "npm run build",
    "test:images": "node -r dotenv/config src/utils/images.test.js",
    "test:cloudinary": "node -r dotenv/config src/tests/cloudinary_upload.test.js",
    "test:banners": "node -r dotenv/config src/tests/banner.tests.js"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "engines": {
    "node": "20.x"
  },
  "devDependencies": {
    "esbuild": "^0.27.0",
    "nodemon": "^3.1.11"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.20.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "prettier": "^3.6.2",
    "stripe": "^20.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "cloudinary": "^1.41.0"
  }
}
